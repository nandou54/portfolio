{
  "name": "portfolio",
  "version": "2.0.0",
  "private": "true",
  "scripts": {
    "dev": "npm run server",
    "prod": "npm run lint && npm run build && npm run server:prod",
    "build": "vite build",
    "server": "node ./server",
    "server:prod": "cross-env NODE_ENV=production node ./server",
    "lint": "npm run lint:format && npm run lint:fix ",
    "lint:fix": "eslint ./src --ext .jsx,.js --quiet --fix --ignore-path ./.eslintignore",
    "lint:format": "prettier  --log-level warn --write \"./**/*.{js,jsx,css,md,json}\" --ignore-path ./.prettierignore"
  },
  "dependencies": {
    "@svgr/plugin-svgo": "^8.1.0",
    "@vitejs/plugin-react": "4.2.1",
    "compression": "1.7.4",
    "cross-env": "7.0.3",
    "express": "4.18.2",
    "html-react-parser": "5.0.10",
    "react": "18.2.0",
    "react-content-loader": "6.2.1",
    "react-dom": "18.2.0",
    "react-intl": "6.5.5",
    "react-toggle-dark-mode": "1.1.1",
    "react-zoom-pan-pinch": "3.3.0",
    "sirv": "2.0.3",
    "vike": "0.4.150",
    "vite": "5.0.10",
    "zustand": "4.4.7"
  },
  "devDependencies": {
    "autoprefixer": "10.4.16",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-prettier": "5.1.0",
    "eslint-plugin-react": "7.33.2",
    "pre-commit": "1.2.2",
    "prettier": "3.1.1",
    "vite-plugin-svgr": "4.2.0"
  },
  "browserslist": [
    "> 0.2%",
    "last 2 version",
    "not dead"
  ],
  "pre-commit": "lint",
  "license": "MIT",
  "type": "module"
}
